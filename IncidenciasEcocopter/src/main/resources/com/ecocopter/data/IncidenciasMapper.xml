<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ecocopter.dao.IncidenciasMapper">
    
    <select id="selectCabecera" resultType="CabIncidencia">
        
        SELECT 
        IDARCHIVO,
        IDNAVE,
        NOMBREARCHIVO,
        CANTREGISTRO,
        ESTADO,
        PESOARCHIVO,
        FEC_CARGA
        FROM  CAB_CARGA
    </select>
</mapper>